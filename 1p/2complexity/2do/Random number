//2. random number
//   random(n)
// -> n integer
// <- one random number in [1-n]

var max = parseInt(prompt("ingresa un lÃ­mite para generar un nÃºmero aleatorio"));
var min = parseInt(prompt("ingresa el mÃ­nimo"));

var resultado = Math.floor(Math.random()*(max - min))+min;
console.log(resultado);

